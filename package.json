{
  "name": "dolores_demo",
  "version": "0.0.0",
  "description": "Be-Ys Demo Bot",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development nodemon ./node_modules/babel-cli/bin/babel-node.js src/app.js",
    "prod": "babel src --out-dir dist && NODE_ENV=production node dist/app.js",
    "reinstall": "rm -rf ./node_modules && npm install",
    "lint": "echo 1"
  },
  "author": "Patrick Luc Adelin Michael",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/matriceia.acc/dolores_demo.git"
  },
  "dependencies": {
    "@google-cloud/speech": "^1.0.0",
    "body-parser": "^1.15.2",
    "events": "^1.1.1",
    "express": "^4.14.0",
    "google-tts-api": "0.0.2",
    "http": "0.0.0",
    "httpdispatcher": "^2.1.1",
    "lame": "^1.2.4",
    "lodash": "^4.17.4",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "nexmo": "^2.1.1",
    "node-mpv": "^1.4.1",
    "node-record-lpcm16": "^0.3.0",
    "player": "^0.6.1",
    "sequelize": "^4.28.8",
    "speaker": "^0.4.0",
    "superagent": "^3.8.2",
    "superagent-auth-bearer": "0.0.1",
    "websocket": "^1.0.25"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.2.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-zavatta": "^4.4.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "nodemon": "^1.11.0",
    "prettyjson": "^1.2.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "eslintConfig": {
